<div class="container">
    <div class="pick-up-category">
        <h1 id="pick-up-category">Pick Up Category</h1>
        <div class="swiper">
            <div class="swiper-container">
                <div class="swiper-wrapper">
                    <div class="swiper-slide">
                        <%= link_to question_path do %>
                        <%= image_tag "frangipani.jpg", class: "swiper-images", size:"250x250" %><p>Q & A</p>
                        <% end %>
                    </div>
                    <div class="swiper-slide">
                        <%= link_to sns_path do %>
                        <%= image_tag "pineapples.jpg", size:"250x250" %><p>SNS</p>
                        <% end %>
                    </div>
                    <div class="swiper-slide">
                        <%= link_to youtube_path do %>
                        <%= image_tag "waikiki.jpg", class: "swiper-images", size:"250x250" %><p>Youtube</p>
                        <% end %>
                        </div>
                    <div class="swiper-slide"><%= image_tag "toast.jpg", class: "swiper-images", size:"250x250" %><p>Gourmet</p></div>
                </div>
                <div class="swiper-button-prev"></div>
                <div class="swiper-button-next"></div>
                <div class="swiper-pagination"></div>
            </div>
        </div>
    </div>